<service>
	<id>DocSpace.Management</id>
	<name>ONLYOFFICE DocSpace Management</name>
	<description>ONLYOFFICE DocSpace Management</description>
	<priority>RealTime</priority>
	<startmode>Automatic</startmode>
	<autoRefresh>false</autoRefresh>
	<onfailure action="restart" delay="10 sec"/>
	<logpath>{APPDIR}Logs</logpath>
	<executable>node</executable>
	<arguments>&quot;{APPDIR}products\ASC.Management\management\server.js&quot;</arguments>
	<log mode="roll-by-size">
  		<sizeThreshold>10240</sizeThreshold>
  		<keepFiles>8</keepFiles>
	</log>
</service>
